{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-15938",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-rc1-15938",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-rc1-15680",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-15715",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-15673",
    "Microsoft.Framework.Logging": "1.0.0-rc1-15634",
    "Microsoft.Framework.Logging.Console": "1.0.0-rc1-15634",
    "Microsoft.Framework.Logging.Debug": "1.0.0-rc1-15634",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-15613",
    "CodeComb.AntiXSS": "1.0.9-rc1-15938",
    "CodeComb.Marked": "1.0.4-rc1-15938",
    "CodeComb.vNext": "1.0.45-rc1-15938",
    "EntityFramework.InMemory": "7.0.0-rc1-15865",
    "EntityFramework.Sqlite": "7.0.0-rc1-15855"
  },

  "commands": {
    "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --config hosting.json",
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --config hosting.json"
  },

  "frameworks": {
    "dnx451": {
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    "dnxcore50": {}
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
