{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-15995",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-rc1-15995",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-rc1-15716",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-15758",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-15707",
    "Microsoft.Extensions.Logging": "1.0.0-rc1-15664",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc1-15664",
    "Microsoft.Extensions.Logging.Debug": "1.0.0-rc1-15664",
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-15647",
    "CodeComb.vNext": "2.0.0-rc1-15995",
    "CodeComb.Marked": "2.0.0-rc1-15995",
    "CodeComb.AntiXSS": "2.0.0-rc1-15995",
    "EntityFramework.InMemory": "7.0.0-rc1-15920",
    "EntityFramework.Sqlite": "7.0.0-rc1-15920"
  },

  "commands": {
    "kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --config hosting.json",
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --config hosting.json"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
